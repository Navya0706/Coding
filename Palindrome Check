class Solution:
    def isPalindrome(self, x: int) -> bool:
        x_str=str(x)
        x_rev=x_str[::-1]
        if x_str==x_rev:
            return True
        else:
            return False    




class Solution:
	def isPalindrome(self, S):
		# code here
		x1=S[::-1]
		if S==x1:
		    return 1
		else:
		    return 0
